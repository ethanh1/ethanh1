//implemantation of function template and class template
 //max,min generic function
 
 template<typename t>
 t max(t a,t b){
 return a>b?a:b;
 }
 
  template<typename t>
 t min(t a,t b){
 return a<b?a:b;
 }
 
 // number to string conversiion and vice versa:
 
 // string to number
 tmplate<typename t>
 t strtonum(const string &p){
 stringstream ss(p);
 t res;
 return ss>>t?t:0;
 }
   // number to string
   
   template<typename t>
   stirng numtostr(t number){
   stringstream ss;
   ss<<number;
   return ss.str();
   }
 
